#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;
int main() {
    int n,i;
    cin>>n;
    for(i=1;i<=n;i++)
    {
        if(i%2!=0)
            cout<<i*i-1<<" ";
        if(i%2==0)
            cout<<i*i-2<<" ";
    }
    return 0;
}
