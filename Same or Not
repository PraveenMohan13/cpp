#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;
int main() {
    
    int n1,n2;
    cin>>n1>>n2;
    int a[n1],b[n2],i,sum=0,sum1=0;
    if(n1==n2)
    {
    for(i=0;i<n1;i++)
    {
      cin>>a[i];
        sum=sum+a[i];
        
    }
    for(i=0;i<n2;i++)
    {
        cin>>b[i];
        sum1=sum1+b[i];
    }   
    if(sum==sum1)
        cout<<"Same";
    else
        cout<<"Not Same"; 
    }
    else
        cout<<"Not Same"; 
        
    return 0;
}
