import java.io.*;
import java.util.*;
public class PM 
{
    public static void main(String[] args) 
    {
        Scanner in = new Scanner(System.in);
        int z;
        double n = in.nextDouble();
        if(n==0)
            System.out.println("0");
        else if(n%9!=0)
        {
            z=(int)(n);
            System.out.println(z%9);
        }
        else if(n%9==0)
            System.out.println("9");
   
        }
    }
