#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;
int main() {
    int n,i;
    cin>>n;
    int a[n];
    bool vi[n+1]={false};
    for(i=0;i<n;i++)
        cin>>a[i];
    for(i=0;i<n;i++)
        vi[a[i]]=true;
    for(i=1;i<=n+1;i++)
    {
       if( vi[i]==false){
        cout<<i;
           break;
       }
    }
}
