#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;
int main() {
    int n;
    cin>>n;
    int a[n][n],i,j;
    for(i=0;i<n;i++)
        for(j=0;j<n;j++)
        cin>>a[i][j];
    
    for(i=0;i<n;i++){
        for(j=n-1;j>=0;j--)
        cout<<a[j][i]<<" ";
    cout<<endl;}
    
    return 0;
}
